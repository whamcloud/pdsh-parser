{
  "name": "@iml/pdsh-parser",
  "version": "1.0.5",
  "description": "PDSH module for parsing pdsh queries",
  "main": "source/index.js",
  "scripts": {
    "eslint": "eslint ./",
    "test": "jest",
    "cover": "NODE_ENV=test yarn test -- --coverage",
    "flow": "flow"
  },
  "publishConfig": {
    "access": "public"
  },
  "pre-commit": [
    "flow",
    "eslint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:whamcloud/pdsh-parser.git"
  },
  "files": [
    "source/*"
  ],
  "jest": {
    "resetMocks": true,
    "resetModules": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!@iml)/"
    ]
  },
  "author": "IML Team",
  "license": "MIT",
  "devDependencies": {
    "@iml/flow-jasmine": "^1.6.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-object-values-to-object-keys": "^1.0.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-entries": "^1.0.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-flowtype": "^3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "flow-bin": "^0.83.0",
    "jest": "^23.6.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.3"
  },
  "dependencies": {
    "@iml/fp": "^8.0.6",
    "@iml/math": "^6.0.1",
    "@iml/obj": "^6.0.3"
  }
}
