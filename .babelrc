{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current",
          "browsers": [
            "last 1 chrome version",
            "last 1 firefox version"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "transform-flow-strip-types",
    "object-values-to-object-keys",
    "transform-object-entries"
  ],
  "sourceMaps": "inline"
}
